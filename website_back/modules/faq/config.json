{
  "tableName": "faq",
  "autoBuild": true,
  "plugins": [
    {
      "name": "sortableList",
      "mode": "overview,reorder,remove",
      "title": "List of FAQ",
      "addBtn": {
        "url": "add",
        "text": "Add"
      },
      "saveBtn": {
        "text": "Save"
      },
      "depth": 1
    },
    {
      "name": "fieldsetform",
      "mode": "add,edit",
      "title": "Add FAQ",
      "setData": "id",
      "plugins": [
        {
          "name": "textfield",
          "mode": "add,edit",
          "label": "Name",
          "columnName": "name",
          "required": true
        },
        {
          "name": "editorTextarea",
          "mode": "add,edit",
          "label": "Text",
          "columnName": "text",
          "escape": [
            "html"
          ]
        }
      ]
    }
  ]
}
